PROCEDURE a(level,dist)
{ IF (level > 0) THEN
  { TURN -45; a(level-1,dist);
    TURN 90; b(level-1,dist);
    TURN -45;
  }
  ELSE
    GO dist;
}

PROCEDURE b(level,dist)
{ IF (level > 0) THEN
  { TURN 45; a(level-1,dist);
    TURN -90; b(level-1,dist);
    TURN 45;
  }
  ELSE
    GO dist;
}

UP; NORTH; GO 400; EAST; GO 150;
DOWN;

a(12, 5);
