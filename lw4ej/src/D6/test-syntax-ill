
//  mein erstes program

VAR alpha;
VAR c;
VAR n;
VAR i;
VAR h j //decl syntax error
VAR m;
VAR t

JUMP 100;
TURN 90;
JUMP 100;
TURN -90;
k = ;   //stmt syntax error
alpha = 77.77;
c = 200;

n=50;

FOR i=1 STEP 1 TO n DO
BEGIN
  TURN alpha;
  c=200*i/n;
  GO c;
  TURN -alpha-90;
  GO sin alpha * c;
  TURN -90;
  GO cos alpha *c;
  TURN 180;
  TURN 2*360/n;
END

